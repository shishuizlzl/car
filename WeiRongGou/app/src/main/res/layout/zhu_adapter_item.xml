<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="235dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/headImage"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:background="@mipmap/zhuceshi"
            android:scaleType="fitXY" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/layRound_one"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <com.che.app.weironggou.util.RoundImageView
                    android:id="@+id/roundImageOne"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:src="@mipmap/zhanwei" />

                <TextView
                    android:id="@+id/roundName_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Bill" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/roundGender_one"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginRight="2dp"
                        android:background="@drawable/man" />

                    <TextView
                        android:id="@+id/roundAge_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:text="15" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layRound_two"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <com.che.app.weironggou.util.RoundImageView
                    android:id="@+id/roundImageTwo"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:src="@mipmap/zhanwei" />

                <TextView
                    android:id="@+id/roundName_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Bill" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/roundGender_two"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginRight="2dp"
                        android:background="@drawable/man" />F

                    <TextView
                        android:id="@+id/roundAge_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:text="15" />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layRound_three"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <com.che.app.weironggou.util.RoundImageView
                    android:id="@+id/roundImageThree"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:src="@mipmap/zhanwei" />

                <TextView
                    android:id="@+id/roundName_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Bill" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/roundGender_three"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginRight="2dp"
                        android:background="@drawable/man" />

                    <TextView
                        android:id="@+id/roundAge_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:text="15" />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layRound_four"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <com.che.app.weironggou.util.RoundImageView
                    android:id="@+id/roundImageFour"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:src="@mipmap/zhanwei" />

                <TextView
                    android:id="@+id/roundName_four"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Bill" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/roundGender_four"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginRight="2dp"
                        android:background="@drawable/man" />

                    <TextView
                        android:id="@+id/roundAge_four"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:text="15" />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layRound_five"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <com.che.app.weironggou.util.RoundImageView
                    android:id="@+id/roundImageFive"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:src="@mipmap/zhanwei" />

                <TextView
                    android:id="@+id/roundName_five"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Bill" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/roundGender_five"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginRight="2dp"
                        android:background="@drawable/man" />

                    <TextView
                        android:id="@+id/roundAge_five"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:text="15" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lookAll"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginRight="10dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:background="@drawable/dankuohao" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:gravity="bottom|center_horizontal">

        <ImageView
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:background="@drawable/jian" />
    </LinearLayout>
    f
</RelativeLayout>